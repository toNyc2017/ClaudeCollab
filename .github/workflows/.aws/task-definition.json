{
    "family": "claude-app",
    "containerDefinitions": [
        {
            "name": "claude-app",
            "image": "463470965515.dkr.ecr.us-east-1.amazonaws.com/yorkville/claude:latest",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512"
}
